  "scripts": {
    "clean": "rm -rf dist coverage .vite",
    "dev": "vite build --watch & chokidar \"dist/**/*\" --ignore \"dist/module.json\" --debounce 300 --await-write-finish -c \"npm run reload\"",
    "dev:full": "npm run link && npm run dev",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "validate": "npm run clean && npm run typecheck && npm run test && npm run build",
    "link": "[ -z \"$FOUNDRY_DATA\" ] && echo 'Set FOUNDRY_DATA to your Foundry Data path' && exit 1 || (mkdir -p \"$FOUNDRY_DATA/Data/modules\" && ln -sfn \"$PWD/dist\" \"$FOUNDRY_DATA/Data/modules/strongholds-and-followers\")",
    "reload": "node scripts/reload-foundry.js",
    "deploy": "npm run build && mkdir -p releases && cd dist && zip -r ../releases/strongholds-and-followers-v$npm_package_version.zip . && cp module.json ../releases/module.json",
    "release": "BUMP=${BUMP:-patch} && npm run validate && npm version $BUMP -m 'chore: release v%s' && git push && git push --tags && npm run deploy",
    "release:patch": "BUMP=patch npm run release",
    "release:minor": "BUMP=minor npm run release",
    "release:major": "BUMP=major npm run release"
  },
